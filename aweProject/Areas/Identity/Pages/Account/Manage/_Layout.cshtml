@{
    Layout = "/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Account verwalten";
}

<div id="OrderCard">
    <div class="card border-dark" id="SiteFormPartialCard">
        <div class="card-header pl-3 pb-0 pt-2">
            <h4>Account verwalten</h4>
        </div>
        <div class="card-body">
            <div>
                <h4>Account-Einstellungen ändern</h4>
                <div class="row">
                    <div class="col-md-1">
                        
                    </div>
                    <div class="col-md-2">
                        <partial name="_ManageNav" />
                    </div>
                    <div class="col-md-9">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @RenderSection("Scripts", required: false)
}
